<?xml version="1.0"?>
<page title="واریز">
    <init>
        <webService address="ws/getTaskByTaskId" value="obj" >
            <webServiceInput name="taskId" ref="urlParams.taskId"/>
        </webService>
    </init>
    <section title="مشخصات تسهیلات">
        <input label="شماره تسک :" type="text" value="obj.taskId"/>
        <input label="شماره مشتری :" type="text" value="obj.customerNumber"/>
        <input label="مبلغ :" type="text" value="obj.amount"/>
        <table label=" حساب های مشتری :" value="obj.savingAccountNumber">
            <input label="شماره حساب" type="text"  />
            <button label="واریز به حساب " >
                <webService address="ws/getUrlByFormKey" value="resultObject">

                </webService>
            </button>

        </table>

        <actions>
            <button label="واریز شد">
                <webService address="ws/cashApproveTask" clearPageOnSuccess="true" >
                    <webServiceInputObject name="obj"/>
                </webService>
            </button>
        </actions>
    </section>
</page>
