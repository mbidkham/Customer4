<?xml version="1.0"?>
<page title="جستجو">
    <init>
        <webService address="ws/getTasks" value="obj" />
    </init>
    <section title="کارتابل" >
        <table label="درخواست ها " value="obj" >
            <input label="نام :" type="text" value="name" disable="true"/>
            <input label="شماره تسک :" type="text" value="taskId" disable="true"/>
            <button label="انجام عملیات" >
                <webService address="ws/getUrlByFormKey" value="resultObject">
                    <webServiceInput name="formKey" ref="currentRow.formKey"/>
                    <afterSuccess>
                        <action type="page" attA="ref" attB="resultObject" attC="currentRow.taskId"/>
                    </afterSuccess>
                </webService>
            </button>
        </table>
    </section>
</page>
